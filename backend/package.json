{
    "name": "back",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "nodemon",
        "check": "prettier --check .",
        "write": "prettier --write .",
        "migrate:up": "npx sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "seed:up": "npx sequelize-cli db:seed:all",
        "seed:undo": "npx sequelize-cli db:seed:undo:all"
    },
    "author": "souto",
    "license": "ISC",
    "description": "",
    "type": "module",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "fs": "^0.0.1-security",
        "jsonwebtoken": "^9.0.2",
        "lorem-ipsum": "^2.0.8",
        "nodemon": "^3.1.4",
        "path": "^0.12.7",
        "prettier": "^3.3.3",
        "randomstring": "^1.3.0",
        "sequelize": "^6.37.3",
        "sequelize-cli": "^6.6.2",
        "sqlite3": "^5.1.7",
        "yup": "^1.4.0"
    },
    "imports": {
        "#/*.js": [
            "./src/*.js"
        ],
        "#models/*.js": [
            "./src/app/models/*.js"
        ],
        "#controllers/*.js": [
            "./src/app/controllers/*.js"
        ],
        "#services/*.js": [
            "./src/app/services/*.js"
        ],
        "#middlewares/*.js": [
            "./src/app/middlewares/*.js"
        ],
        "#helper/*.js": [
            "./src/helpers/*.js"
        ],
        "#routes/*.js": [
            "./src/routes/*.js"
        ],
        "#config/*.js": [
            "./src/config/*.js"
        ],
        "#test/*.js": [
            "./src/test/*.js"
        ],
        "#validators/*.js": [
            "./src/app/validators/*.js"
        ],
        "#dtos/*.js": [
            "./src/app/dtos/*.js"
        ],
        "#database/*.js": [
            "./src/database/*.js"
        ],
        "#helpers/*.js": [
            "./src/helpers/*.js"
        ]
    },
    "devDependencies": {
        "babel-register": "^6.26.0"
    }
}
